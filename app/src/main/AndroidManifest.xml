            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CctvActivity" android:label="Cctv" android:parentActivityName=".MainActivity"/>
        <activity android:name=".JoinActivity" android:label="Join" android:parentActivityName=".MainActivity"/>
